<tr id="<%= dom_id(user, :table_form) %>" class="table-form" data-url="<%= url_for([contest, :orgcom, user]) %>">
  <td style=""><%= user_counter + 1 %></td>

  <%= fields_for user do |f| %>
    <td><%= f.text_field :name %></td>

    <td>
      <% if contest.cities.present? %>
        <%= f.select :city, contest.cities %>
      <% else %>
        <%= f.text_field :city %>
      <% end %>
    </td>

    <td>
      <% if contest.institutions.present? %>
        <%= f.select :institution, contest.institutions %>
      <% else %>
        <%= f.text_field :institution %>
      <% end %>
    </td>

    <td><%= f.select :grade, Grade.all.map { [_1.name, _1.id] } %></td>
  <% end %>

  <td><input type="button" class="hidden" /></td>
</tr>
